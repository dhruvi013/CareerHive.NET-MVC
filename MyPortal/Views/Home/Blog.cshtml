            .blog .blog-item a.btn {
                position: relative;
                padding: 0 0 0 35px;
                font-size: 14px;
                font-weight: 400;
                letter-spacing: 1px;
                color: #0796fe;
                transition: .3s;
            }

                .blog .blog-item a.btn:hover {
                    color: #092a49;
                }

                .blog .blog-item a.btn::before {
                    position: absolute;
                    content: "";
                    width: 30px;
                    height: 2px;
                    top: 10px;
                    left: 0;
                    background: #0796fe;
                }

            .blog .owl-nav {
                position: relative;
                width: 100%;
                margin-top: -30px;
                display: flex;
                justify-content: center;
            }

                .blog .owl-nav .owl-prev,
                .blog .owl-nav .owl-next {
