                    width: 100px;
                    display: flex;
                    align-items: center;
                    justify-content: center;
                    color: #0796fe;
                    font-size: 25px;
                    transition: .3s;
                }

                .blog .owl-nav .owl-prev {
                    padding-right: 80px;
                }

                .blog .owl-nav .owl-next {
                    padding-left: 80px;
                }

                    .blog .owl-nav .owl-prev:hover,
                    .blog .owl-nav .owl-next:hover {
                        color: #092a49;
                    }

                    .blog .owl-nav .owl-prev::after,
                    .blog .owl-nav .owl-next::after {
                        position: absolute;
                        content: "";
                        width: 80px;
                        height: 3px;
                        top: 11px;
                        background: #0796fe;
                        transition: .3s;
                    }

                .blog .owl-nav .owl-prev::after {
                    left: 9px;
                }

                .blog .owl-nav .owl-next::after {
                    right: 9px;
                }

                .blog .owl-nav .owl-prev:hover::after {
                    background: #092a49;
                }

                .blog .owl-nav .owl-next:hover::after {
